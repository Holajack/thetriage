{
  "expo": {
    "name": "HikeWise",
    "slug": "thetriage",
    "version": "1.7.0",
    "runtimeVersion": "1.7.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-ios-white-1024.png",
    "scheme": "hikewise",
    "updates": {
      "enabled": false,
      "checkAutomatically": "NEVER",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/8c921112-45b4-48cf-91c3-a1326803d706"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hikewise.app",
      "icon": "./assets/icon-ios-white-1024.png",
      "buildNumber": "14",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses camera for document scanning",
        "NSMicrophoneUsageDescription": "This app uses microphone for voice features"
      },
      "associatedDomains": []
    },
    "android": {
      "package": "com.hikewise.app",
      "versionCode": 19,
      "intentFilters": []
    },
    "extra": {
      "eas": {
        "projectId": "8c921112-45b4-48cf-91c3-a1326803d706"
      }
    },
    "plugins": [
      "expo-localization",
      "expo-asset",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera for QR code scanning and document capture."
        }
      ],
      "expo-web-browser"
    ],
    "owner": "hollaj"
  }
}
