{
  "expo": {
    "name": "Triage System",
    "slug": "triage-system",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/triage-app-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B4A3A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.triagesystem.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/triage-app-logo.png",
        "backgroundColor": "#1B4A3A"
      },
      "package": "com.triagesystem.app",
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#1B4A3A",
        "mdpi": "./assets/splash-icon.png",
        "hdpi": "./assets/splash-icon.png",
        "xhdpi": "./assets/splash-icon.png",
        "xxhdpi": "./assets/splash-icon.png",
        "xxxhdpi": "./assets/splash-icon.png"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "53.0.0",
    "plugins": [
      "expo-localization",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1B4A3A"
        }
      ]
    ],
    "scheme": "triage-system"
  }
}
